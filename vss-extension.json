{
  "manifestVersion": 1,
  "version": "0.1.18",
  "name": "Unofficial Beckhoff TwinCAT 3 preview extension",
  "description": "An extension for previewing TwinCAT files in repos",
  "publisher": "dfreiberger",
  "demands": ["api-version/3.0"],
  "categories": ["Azure Repos"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "icons": {
    "default": "img/world.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "tc-preview",
      "type": "ms.vss-code-web.content-renderer",
      "targets": [
          "ms.vss-code-web.content-renderer-collection"
      ],
      "properties": {
          "uri": "dist/preview/preview.html",
          "fileExtensions": [
              "TcPOU",
              "TcDUT",
              "TcGVL",
              "TcIO"
          ],
          "mimeTypes": [
              "text/html"
          ],
          "defaultBehavior": "showRenderedContent",
          "registeredObjectId": "tc-preview"
      }
    },
    {
      "id": "hello-world-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "Sample hub group",
      "targets": ["ms.vss-web.project-hub-groups-collection"],
      "properties": {
        "name": "Hello World!",
        "icon": {
          "dark": "img/world.png",
          "light": "img/world.png"
        }
      }
    },
    {
      "id": "default-hub",
      "type": "ms.vss-web.hub",
      "targets": [".hello-world-hub-group"],
      "properties": {
        "uri": "dist/hub/hub.html"
      }
    }
  ]
}
